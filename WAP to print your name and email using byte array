import java.io.*;
class Geeks {
    public static void main(String[] args) {
       
        byte[] byteArray = { 115 ,104,114,117,116,105 };
        String str3 = new String(byteArray);
        System.out.println("String from byte array: " + str3);
         byte[] byteArray2 = { 115 ,104,114,117,116,105 ,64,103,109,97,105,108,46,99,111,109 };
        String str4 = new String(byteArray2);
        System.out.println("String from byte array2: " + str4);
    }
}
