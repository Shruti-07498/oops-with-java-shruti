class Geeks{
    String name;
    int id;
    
    Geeks(String name, int id){
        this.name=name;
        this.id=id;
    }
}
class GFG
{
    public static void main(String []args)
    {
        Geeks geek1 =new Geeks("shruti",77);
        System.out.println("Geek Name:"+geek1.name+"and Geek Id:"+geek1.id);
    }
}
